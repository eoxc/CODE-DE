<div class="panel-heading">
  <h3 class="panel-title">
    <a role="button" data-toggle="collapse" href="#collapse-time-filter">{{t 'Time Filter'}} <span class="caret"></span></a>
  </h3>
</div>
<div id="collapse-time-filter" class="panel-body collapse in">
  <div id="map-time-wrapper" class="form-group">
    <label for="">{{t 'Using same time as for map'}}</label>
    <div class="form-group">
      <div class="input-group">
        <input type='text' disabled="true" class="form-control input-sm map-time-start" />
        <span class="input-group-addon">-</span>
        <input type='text' disabled="true" class="form-control input-sm map-time-end" />
      </div>
    </div>
  </div>

  <div class="form-group">
    <label for="">{{t 'Start'}}</label>
    <div class='input-group datetime start'>
      <input type='text' class="form-control input-sm" />
      <span class="input-group-addon">
        <i class="fa fa-calendar" aria-hidden="true"></i>
      </span>
    </div>
  </div>

  <div class="form-group">
    <label for="">{{t 'End'}}</label>
    <div class='input-group datetime end'>
      <input type='text' class="form-control input-sm" />
      <span class="input-group-addon">
        <i class="fa fa-calendar" aria-hidden="true"></i> 
      </span>
    </div>
  </div>

  <div class="form-group time-buttons">
    <div class="btn-group btn-group-justified" role="group" aria-label="...">
      <a href="#" role="button" class="btn btn-default btn-sm tool tool-show-time">{{t 'Zoom to Selection'}}</a>
      <a href="#" role="button" class="btn btn-danger btn-sm tool tool-clear-time">{{t 'Clear Selection'}}</a>
    </div>
  </div>
</div>
